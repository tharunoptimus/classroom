                        img(src=userLoggedIn.profilePic, alt="Profile Pic")
            .content
                block content
        block modals

        #changeClassSettingsModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='changeClassSettingsLabel' aria-hidden='true')
            .modal-dialog(role='document')
                .modal-content
                    .modal-header
                        h5#changeClassSettingsLabel.modal-title Class Settings
                    .modal-body.createTaskModalBody
                        input#changeClassName(placeholder="Class Name")
                    .modal-footer
                        button.btn.btn-secondary(type='button' data-dismiss='modal') Close
                        button#deleteClassButton.btn.btn-danger Delete Class
                        button#confirmChangeClassButton.btn.btn-primary(type='button') Change
        
        #hamburgerMenu.modal.fade(tabindex='-1' role='dialog' aria-labelledby='hamburgerMenuLabel' aria-hidden='true')
            .modal-dialog(role='document')
                .modal-content
                    .modal-header
                        h5#hamburgerMenuLabel.modal-title Menu
                    .modal-body.justifySpaceEvenly
                        button.btn.btn-outline-success.home(type="button" ) Home
                        button.btn.btn-outline-info.calendar(type="button" ) Calendar
                        button.btn.btn-outline-danger.logout(type="button" ) Logout
                    .modal-footer
                        button.btn.btn-secondary(type='button' data-dismiss='modal') Cancel

        script(src='/js/jquery.js')
        script(src='/js/popover.js')
        script(src='/js/bootstrap.js')
        
        block scripts 
        
        script(src='/js/common.js')
        script.
            if('serviceWorker' in navigator) {
                window.addEventListener('load', () => {
                    navigator.serviceWorker.register('serviceworker.js')
                        .then((reg) => console.log('Success: ', reg.scope))
                        .catch((err) => console.log('Failure: ', err));
                })
            }